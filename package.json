{
  "name": "ifdian",
  "version": "0.0.6",
  "description": "爱发电SDK",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "ky": "^1.5.0",
    "super-fast-md5": "^1.0.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "clean": true,
    "dts": true,
    "treeshake": true,
    "shims": true,
    "minify": true
  },
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.4",
    "tsup": "^8.2.3"
  }
}
